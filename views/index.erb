<div class="heading">
  <span class="btn message"><a href="/new">Create a new memo</a></span>
</div>
<div class="memo-list">
  <ul>
    <% @memos.each_with_index do |memo, i| %>
   <li class="memo-title-link">
      <a href="/show/<%= memo[:id] %>">
      <%= memo[:title] %></a>
   </li>
   <% end %>
  </ul>
</div>

